<Window x:Class="Lec85.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lec85"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtKey" HorizontalAlignment="Left" Height="23" Margin="310,59,0,0" TextWrapping="Wrap" Text="Key" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="txtValue" HorizontalAlignment="Left" Height="23" Margin="310,97,0,0" TextWrapping="Wrap" Text="Value" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btRemove" Content="Remove" HorizontalAlignment="Left" Margin="328,149,0,0" VerticalAlignment="Top" Width="75" Click="btRemove_Click"/>
        <Button x:Name="btAdd" Content="Add" HorizontalAlignment="Left" Margin="210,149,0,0" VerticalAlignment="Top" Width="75" Click="btAdd_Click"/>
        <Button x:Name="btShow" Content="Show" HorizontalAlignment="Left" Margin="450,149,0,0" VerticalAlignment="Top" Width="75" Click="btShow_Click"/>

    </Grid>
</Window>
